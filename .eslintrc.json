{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jasmine": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "no-alert": 0,
        "no-prototype-builtins": 0,
        "no-return-assign": 0,
        "import/prefer-default-export":0
    }
}
